{
  "run": {
    "timeout": "10m"
  },
  "linters": {
    "enable":  [
      "errcheck",
      "gosimple",
      "govet",
      "ineffassign",
      "staticcheck",
      "typecheck",
      "unused",
      "asasalint",
      "containedctx",
      "whitespace",
      "containedctx",
      "contextcheck",
      "noctx",
      "predeclared",
      "gofmt",
      "ginkgolinter",
      "goconst",
      "goimports",
      "unconvert",
      "tenv"
    ]
  },
  "linters-settings": {
    "goconst": {
     "ignore-tests": true
    }
  }
}